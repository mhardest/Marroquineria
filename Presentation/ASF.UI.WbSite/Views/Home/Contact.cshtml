@{
    ViewBag.Title = "Contacto";
    ViewBag.Description = "Escribinos tu consulta y nos contactaremos a la brevedad";
    ViewBag.Author = "Matias Martin";
    ViewBag.ReferrerMode = ReferrerMode.OriginWhenCrossOrigin;
    ViewBag.TwitterCard = new SummaryLargeImageTwitterCard("@Mhardest");
    ViewBag.OpenGraph = new OpenGraphProfile(
        ViewBag.Title,
        new OpenGraphImage(Url.AbsoluteContent("~/content/icons/open-graph-1200x630.png")) { Height = 630, Type = ContentType.Png, Width = 1200 })
    {
        Determiner = OpenGraphDeterminer.Blank,
        FirstName = "Matias",
        Gender = OpenGraphGender.Male,
        LastName = "Martin",
        SiteName = Application.Name,
        Username = "Matias Martin"
    };
}

<header class="page-header">
    <h1><span aria-hidden="true" class="fa fa-phone"></span> @ViewBag.Title</h1>
</header>

<div class="row">
    <section class="col-md-9" role="region">
        <p>@ViewBag.Description</p>
    </section>
    <aside class="col-md-3" role="complementary">
        <address>
            Bernardo de Irigoyen 696<br>
            San Isidro, B1609BFJBFK<br>
            <abbr title="Telefono">Tel:</abbr>
            011 4737-5313
        </address>
        <address>
            <strong>Info:</strong>   <a href="mailto:Support@example.com">Support@example.com</a><br />
            <strong>Ventas:</strong> <a href="mailto:Marketing@example.com">Marketing@example.com</a>
        </address>
    </aside>
</div>